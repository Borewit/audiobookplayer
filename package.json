{
  "name": "audiobookplayer",
  "version": "0.1.4",
  "description": "A new audiobook player",
  "author": "Duncan Haig",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-brands-svg-icons": "^5.1.0-11",
    "@fortawesome/free-regular-svg-icons": "^5.1.0-11",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "deep-equal": "^1.0.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "electron-updater": "^3.0.3",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "fuse.js": "^3.2.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lokijs": "^1.5.3",
    "music-metadata": "^2.4.2",
    "nconf": "^0.10.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.3.0",
    "react-id-generator": "^0.1.6",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.8.2",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "electron": "set NODE_ENV=development && electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && set NODE_ENV=development && electron .",
    "pack": "yarn build && electron-builder --dir",
    "dist": "yarn build && electron-builder",
    "dist:linux": "yarn build && electron-builder --linux",
    "dist:mac": "yarn build && electron-builder --macos",
    "dist:win": "yarn build && electron-builder --win",
    "release": "electron-builder --publish always",
    "eb-start": "electron-builder start",
    "eb-create-certs": "electron-builder create-self-signed-cert",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1"
  },
  "build": {
    "appId": "app.AudioBookPlayer",
    "productName": "Audio Book Player",
    "copyright": "Copyright Â© 2018 Duncan Haig",
    "files": [
      "build/**/*",
      "src/services/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "icon": "public/icon.ico",
      "publish": {
        "provider": "s3",
        "bucket": "audiobookplayer",
        "encryption": "AES256",
        "acl": "public-read",
        "path": "/builds/win/"
      }
    },
    "linux": {
      "target": "AppImage",
      "category": "Audio",
      "publish": {
        "provider": "s3",
        "bucket": "audiobookplayer",
        "encryption": "AES256",
        "acl": "public-read",
        "path": "/builds/linux/"
      }
    },
    "mac": {
      "category": "public.app-category.music",
      "icon": "public/icon.icns",
      "publish": {
        "provider": "s3",
        "bucket": "audiobookplayer",
        "encryption": "AES256",
        "acl": "public-read",
        "path": "/builds/mac/"
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
